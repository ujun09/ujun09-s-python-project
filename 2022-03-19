l = []
sum = 0
avg = 0
while True :
    a = int(input())
    l.append(a)
    if a >= 100:
        break
for i in l:
    sum += i
avg = sum / (len(l))
print(sum)
print("%.1f"%(avg))

l = []
b = 0
c = 0
for i in range(10):
    a = int(input())
    l.append(a)
for j in l:
    if j % 3 == 0:
        b +=1
for k in l:
    if k % 5 == 0:
        c += 1
print("Multiples of 3 : %d"%(b))
print("Multiples of 5 : %d"%(c))

